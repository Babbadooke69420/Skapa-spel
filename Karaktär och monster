import random as rand
level = 1
def random_nummer(välj_väg):
    lst = list(range(1,4))
    lst = rand.choice(lst)
    return(lst)


import random as rand
svärd = 5
pilbåge = 3
yxa = 8


print('skapa din karaktär')
while True:
    input('Välj namn på din karaktär ->')
    break
while True:
    kön = input('Välj kön på din karaktär ->')
    if kön == "man" or kön == "kvinna":
        break
print('Alternativen är beige brun och gul')
while True:
    färg = input('välj färg på din karaktär->')
    if färg == "brun" or färg == "beige" or färg == "gul":
        break
    else:
        continue
print( 'Alternativ, pilbåge, svärd, yxa->')
print( 'Svärdet gör 5 skada och har 5 i snabbhet')
print( 'Pilbågen gör 3 i skada och har 7 i snabbhet')
print( 'Yxan gör 8 i skada och har 2 i snabbhet')
while True:
    vapen = input('Välj vapen till din karaktär ->')
    if vapen == "pilbåge":
        attack = 3
        break
    elif vapen == "svärd":
        attack = 5
        break
    elif vapen == "yxa":
        attack = 8
        break
    else:
        print("Skriv ett korrekt vapen")
print('Din karaktär är färdig')
print(vapen)

landskap = input("Det är dags att välja vart du vill utforska, vill du resa i Italien, I "
                 "i Egypten, E eller i Kina, K ?  -->  ")

if landskap  == "E":
    print(" Du valde att resa i Egypten, ett klokt val!")
elif landskap == "I" :
    print(" Du valde att resa i Italien, spännande ! ")
elif landskap == "K":
    print("Du valde att utforska Kina, en otrolig resa väntar dig")

def kina():
    while True:
        välj_väg = input("Vill du gå höger, vänster eller framåt?")
        if välj_väg == "höger":
            lst = random_nummer(välj_väg)
            print(lst)
            if lst == 1:
                print()
            elif lst == 2:
                print("de")
            elif lst == 3:
                print("hejdå")
        elif välj_väg == "vänster":
            lst = random_nummer(välj_väg)
            print(lst)
            if lst == 1:
                print("här ska det vara en finktion")
            elif lst == 2:
                print("här också")
            elif lst == 3:
                print("här med")
            
        elif välj_väg == "framåt":
            lst = random_nummer(välj_väg)
            print(lst)
            if lst == 1:
                print("b")
            elif lst == 2:
                print("h")
            elif lst == 3:
                print("g")
        speed = 10 - attack
        hp_monster = rand.randint(1,10)
        print(f"Monstret har {hp_monster} hp")

    def monster():
        while True:
            if attack > hp_monster:
                print('du besegrar monstret')
                level += 1
            else:
                nummer = rand.randint(1, 3)
                global speed
                chans_att_fly = rand.randint(1, 10-speed)
                if chans_att_fly == nummer:
                    print('Du lyckades att fly')
                else:
                    print('Du förlorade')
            break

    def kista():
        import random

        Skattkista = ["horn", "sköld", "mat", "kniv", "osynlighetsdryck"]

        sannolikhet = [0.1, 0.3, 0.4, 0.15, 0.05]

        föremål = random.choices(Skattkista, sannolikhet)[0]

        print(f"Du hittade en {föremål}!")
        if föremål == "horn":
            print('Hornet kan användas när du möter en boss. Det kallar på en arme av spindlar som gör ganska mycket skada på bossen')
        elif föremål == "sköld":
            print('Skölden gör så att du får 5 extra hp. Det kan vara till nytta i alla fajter mot både bossar och monster')
        elif föremål == "mat":
            print('Maten hjälper dig att heala upp/fylla på ditt hp om du tagit skada')
        elif föremål == "kniv":
            print('Kniven gör så att du gör 3 extra skada. Det kan användas mot både bossen och monster.')
        elif föremål == "osynlighetsdryck":
            print('Osynlighetsdrycken gör så att du kan fly från en bossfajt om du håller på att dö. Den är väldigt ovanlig och kan vara till stor nytta.')

    def skapa_fälla():
        fälla_skada = random.randint(1, 10)
        print("Helvete, du föll i en fälla och tog skada:", fälla_skada)

    def indra():
        Attack = 7
        Hp = 20
        Möjligt_Hp = list(range(25, 35))
        Indra_Hp = rand.choice(Möjligt_Hp)
        möjlig_skada = list(range(1, 8))
        print('Du har hamnat i en bossfajt!')
        print('Du kan välja att fly omedelbart eller att slåss och riskera att dö.')
        while True:
            Indra_Hp = Indra_Hp - Attack
            print(f'Monstret har {Indra_Hp} hp kvar')
            if Indra_Hp == 0 or Indra_Hp <=0:
                print('Bra jobbat! Du besegrade Indra och klarade spelet')
                break
            else:
                continue
            Attack_Indra = rand.choice(möjlig_skada)
            print(f'Indra gjorde{Attack_Indra} skada')
            Hp = Hp - Attack_Indra
            if Hp == 0 or Hp <=0:
                print('Du dog')
                print('GAME OVER')
                break
            else:
                continue
    def bossfajt()
        if level == 10:
            print('Du är nu i level 10 och kan möta en boss! Har du allt som krävs för att möta bossen')
            svar = input("Om du vill möta bossen, tryck ja, j, om inte tryck nej, n ")
                if svar == "ja" or svar == "j"
                  print('Nu har du valt att starta en bossfajt!')
 

def Egypten():
    while True:
        välj_väg = input("Vill du gå höger, vänster eller framåt?")
        if välj_väg == "höger":
            lst = random_nummer(välj_väg)
            print(lst)
            if lst == 1:
                print()
            elif lst == 2:
                print("de")
            elif lst == 3:
                print("hejdå")
        elif välj_väg == "vänster":
