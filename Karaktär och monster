while True:
    landskap = input("Det är dags att välja vart du vill utforska, vill du resa i Italien, I "
                 "i Egypten, E eller i Kina, K ?  -->  ")
    if landskap  == "E":
        print(" Du valde att resa i Egypten, ett klokt val!")
        break
    elif landskap == "I" :
        break
        print(" Du valde att resa i Italien, spännande ! ")
    elif landskap == "K":
        print("Du valde att utforska Kina, en otrolig resa väntar dig")
        break
    else:
        print("Välj ett landskap som finns")
        continue


speed = 10 - attack
def monster():
    
    hp_monster = rand.randint(1,10)
    print(f"Monstret har {hp_monster} hp")
    while True:
        if attack > hp_monster:
            (print('du besegrar monstret'))
            global level
            level +=1
            return print("Din nya level", (level))
        else:
            nummer = rand.randint(1, 3)
            global speed
            chans_att_fly = rand.randint(1, 10-speed)
            if chans_att_fly == nummer:
               print('Du lyckades att fly')
            else:
                print('Du förlorade, men du lever för att kämpa en annan dag')
                global hp
                hp = hp - 2
                return print("Ditt hp är nu",(hp))
                
                       
        break

def skapa_fälla():
    fälla_skada = rand.randint(1, 3)
    print("Helvete, du föll i en fälla och tog skada:", fälla_skada)
    global hp
    hp = hp - fälla_skada
    return(hp)




def kista():
    import random

    Skattkista = ["horn", "sköld", "mat", "kniv", "osynlighetsdryck"]

    sannolikhet = [0.1, 0.3, 0.4, 0.15, 0.05]

    föremål = random.choices(Skattkista, sannolikhet)[0]

    print(f"Du hittade en {föremål}!")
    if föremål == "horn":
        inv.append("horn")
        global attack
        attack = attack + 2
    
        
        print('Hornet kan användas när du möter en boss. Det kallar på en arme av spindlar som gör ganska mycket skada på bossen')
        return(print(attack))
    elif föremål == "sköld":
        print('Skölden gör så att du får 5 extra hp. Det kan vara till nytta i alla fajter mot både bossar och monster')
        global hp
        inv.append("sköld")
        hp = hp + 5
    elif föremål == "mat":
        print('Maten hjälper dig att heala upp/fylla på ditt hp om du tagit skada')
        hp = hp + 2
        return("Ditt hp är nu", hp)
    elif föremål == "kniv":
        print('Kniven gör så att du gör 3 extra skada. Det kan användas mot både bossen och monster.')
        inv.append("kniv")
        attack = attack + 3
    elif föremål == "osynlighetsdryck":
        print('Osynlighetsdrycken gör så att du kan fly från en bossfajt om du håller på att dö. Den är väldigt ovanlig och kan vara till stor nytta.')


    

def Hades():
    Attack = 7
    Hp = 20
    Möjligt_Hp = list(range(25, 35))
    Hades_Hp = rand.choice(Möjligt_Hp)
    möjlig_skada = list(range(1, 8))
    print('Du har hamnat i en bossfajt!')
    print('Du kan välja att fly omedelbart eller att slåss och riskera att dö.')
    while True:
        Hades_Hp = Hades_Hp - Attack
        print(f'Monstret har {Hades_Hp} hp kvar')
        if Hades_Hp == 0 or Hades_Hp <=0:
            print('Bra jobbat! Du besegrade Hades och klarade spelet')
            break
        else:
            continue
        Attack_Hades = rand.choice(möjlig_skada)
        print(f'Hades gjorde{Attack_Hades} skada')
        Hp = Hp - Attack_Hades
        if Hp == 0 or Hp <=0:
            print('Du dog')
            print('GAME OVER')
            break
        else:
            continue  

    
import random as rand

def indra():
    Attack = 7
    Hp = 20
    Möjligt_Hp = list(range(25, 35))
    Indra_Hp = rand.choice(Möjligt_Hp)
    möjlig_skada = list(range(1, 8))
    print('Du har hamnat i en bossfajt!')
    print('Du kan välja att fly omedelbart eller att slåss och riskera att dö.')
    while True:
        Indra_Hp = Indra_Hp - Attack
        print(f'Monstret har {Indra_Hp} hp kvar')
        if Indra_Hp == 0 or Indra_Hp <=0:
            print('Bra jobbat! Du besegrade Indra och klarade spelet')
            return   
            break
        
        else:
            continue
        Attack_Indra = rand.choice(möjlig_skada)
        print(f'Indra gjorde{Attack_Indra} skada')
        Hp = Hp - Attack_Indra
        if Hp == 0 or Hp <=0:
            print('Du dog')
            print('GAME OVER')
            break
        else:
            continue 

    
import random as rand

def Sekhmet():
    Attack = 7
    Hp = 20
    Möjligt_Hp = list(range(25, 35))
    Sekhmet_Hp = rand.choice(Möjligt_Hp)
    möjlig_skada = list(range(1, 8))
    print('Du har hamnat i en bossfajt!')
    print('Du kan välja att fly omedelbart eller att slåss och riskera att dö.')
    while True:
        Sekhmet_Hp = Sekhmet_Hp - Attack
        print(f'Monstret har {Sekhmet_Hp} hp kvar')
        if Sekhmet_Hp == 0 or Sekhmet_Hp <=0:
            print('Bra jobbat! Du besegrade Sekhmet och klarade spelet')
            break
        else:
            continue
        Attack_Sekhmet = rand.choice(möjlig_skada)
        print(f'Sekhmet gjorde{Attack_Sekhmet} skada')
        Hp = Hp - Attack_Indra
        if Hp == 0 or Hp <=0:
            print('Du dog')
            print('GAME OVER')
            break
        else:
            continue

        
    

    





    



print("Ditt hp är", hp)
print("Din level är", level)
print("Din attack är", attack)
while hp > 0:
    if hp <= 0:
        print("Game over")
    
    stats = input("tryck q för inv och stats tryck något annat för att fortsätta")
    if stats == "q":
        print(inv, hp, level, attack)
   
    välj_väg = input("Vill du gå höger, h, vänster, v eller framåt, f?")
    if välj_väg == "höger" or "h":
        lst = random_nummer(välj_väg)
    
    
        if lst == 1:
            print(monster())
            


        elif lst == 2:
            print(skapa_fälla())
            
        elif lst == 3:
            print(kista())
        

    elif välj_väg == "vänster" or "v":

        lst = random_nummer(välj_väg)
        

        if lst == 1:
            print(monster())

        elif lst == 2:
            print(skapa_fälla())

        elif lst == 3:
            print(kista())
            

    elif välj_väg == "framåt" or "f":

        
        lst = random_nummer(välj_väg)

        print(lst)

        if lst == 1:
            print(monster())

        elif lst == 2:
            print(skapa_fälla())

        elif lst == 3:
            print(kista())
    if level == 3:
        print(indra())
    
    





    

        







    

        



 
