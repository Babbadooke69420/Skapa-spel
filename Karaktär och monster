print("Ditt hp är", hp)

while hp > 0:
    välj_väg = input("Vill du gå höger, vänster eller framåt?")
    if välj_väg == "höger":
        lst = random_nummer(välj_väg)
        print(lst)

        if lst == 1:
            print(monster())
            

        elif lst == 2:
            print(skapa_fälla())
            
        elif lst == 3:
            print(kista())

    elif välj_väg == "vänster":
        lst = random_nummer(välj_väg)
        print(lst)

        if lst == 1:
            print(monster())

        elif lst == 2:
            print(skapa_fälla())

        elif lst == 3:
            print(kista())
            

    elif välj_väg == "framåt":
        lst = random_nummer(välj_väg)
        print(lst)

        if lst == 1:
            print(monster())

        elif lst == 2:
            print(skapa_fälla())

        elif lst == 3:
            print(kista())



